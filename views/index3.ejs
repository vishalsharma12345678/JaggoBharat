<!DOCTYPE html>
<html>
<head>
<style>

 .grid-container-main{
        display: flex;
        justify-content: center;
    }

.grid-container {
  display: grid;
  grid-template-columns: repeat(4, auto); 
  grid-gap: 10px;
/*  background-color: #dce2e7;*/
  padding: 10px;
  width: 95vw;
}


.grid-container > div {
  background-color: rgba(255, 255, 255, 0.8);
  text-align: center;
}

.item6 { /* Centered item */
  grid-column: 2 / span 2;  /*Starts at column 5 and spans 2 columns*/
  grid-row: span 2;
}

img{
    width: 100%;
    height: 100%;
}
</style>
</head>
<body>

<div class="grid-container-main">
  <div class="grid-container">
    <div class="item1"><img src="Images/home.jpeg"></div>
    <div class="item2"><img src="Images/a1.jpeg"></div>
    <div class="item3"><img src="Images/h5.jpeg"></div>  
    <div class="item4"><img src="Images/h3.jpeg"></div>
    <div class="item5"><img src="Images/h6.jpeg"></div> <!-- Centered item -->
    <div class="item6"><img src="Images/t5.jpeg"></div>
    <div class="item7"><img src="Images/m1.jpg"></div>
    <div class="item8"><img src="Images/m5.jpg"></div>
    <div class="item9"><img src="Images/m6.jpg"></div>
    <div class="item10"><img src="Images/m2.jpg"></div>
    <div class="item11"><img src="Images/m4.jpg"></div>
    <div class="item12"><img src="Images/m3.jpg"></div>
    <div class="item13"><img src="Images/m7.jpg"></div>
  </div>
</div>
</body>
</html>
